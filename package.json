{
  "scripts": {
    "start": "cross-env NODE_ENV=development PACKAGE_ENV=dev useProxy=mock webpack-dev-server --config ./config/webpack.config.ts",
    "qa": "cross-env NODE_ENV=development PACKAGE_ENV=qa useProxy=qa webpack-dev-server --config ./config/webpack.config.ts",
    "pre": "cross-env NODE_ENV=development PACKAGE_ENV=pre useProxy=pre webpack-dev-server --config ./config/webpack.config.ts",
    "build:qa": "cross-env NODE_ENV=production PACKAGE_ENV=qa webpack --config ./config/webpack.config.ts",
    "build:pre": "cross-env NODE_ENV=production PACKAGE_ENV=pre webpack --config ./config/webpack.config.ts",
    "build": "cross-env NODE_ENV=production PACKAGE_ENV=prod webpack --config ./config/webpack.config.ts",
    "type-check": "tsc --noEmit",
    "tsc": "tsc",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-activation": "^0.12.4",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.6",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "postcss-px-to-viewport": "^1.1.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "tsc-files global.d.ts --noEmit"
    ]
  }
}